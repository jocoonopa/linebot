{
    "name": "jocoonopa/linebot",
    "description": "Let using line-bot-sdk be much more comfortable in Laravel5.5+",
    "type": "plugin",
    "require": {
        "illuminate/support": ">=4.0",
        "linecorp/line-bot-sdk": "^6.3"
    },
    "version": "6.3.0",
    "keywords": ["Laravel", "LINEBOT", "LINE"],
    "license": "MIT",
    "authors": [
        {
            "name": "jocoonopa",
            "email": "jocoonopa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "jocoonopa\\LINEBot\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jocoonopa\\LINEBot\\LINEBotServiceProvider"
            ],
            "aliases": {
                "LINEBot": "jocoonopa\\LINEBot\\Facades\\LINEBot"
            }
        }
    }
}
